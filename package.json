{
  "name": "react-komposer",
  "version": "1.13.1-ts",
  "description": "Compose React containers and feed data into components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-komposer.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepare": ". ./scripts/prepublish.sh",
    "lint": "eslint ./lib",
    "lintfix": "eslint ./lib --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch"
  },
  "react-native": "./dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "browserify": "12.x.x",
    "chai": "3.x.x",
    "enzyme": "^3.11.0",
    "eslint": "^8.36.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "3.x.x",
    "exposify": "0.5.x",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rx": "4.x.x",
    "uglifyify": "^5.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "hoist-non-react-statics": "1.x.x",
    "invariant": "2.x.x",
    "shallowequal": "0.2.x",
    "mobx": "^2.3.4",
    "prop-types": "^15.8.1"
  }
}
